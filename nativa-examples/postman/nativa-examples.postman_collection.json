            "url": { "raw": "{{baseUrl}}/api/customers", "host": ["{{baseUrl}}"], "path": ["api", "customers"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"Asha\",\n  \"lastName\": \"K\",\n  \"email\": \"asha@example.com\",\n  \"phone\": \"+1-555\"\n}"
